<!--show all user-->
<div>
  <h1 class="text-2xl"> all users</h1>
  <ul>
    <% @users.each do |user| %>
      <li>
        <div class=" flex flex-col items-center container mb-5 border-2 ">
          <%= user.username %>
          <%= user.email %>
          <%= user.id %> <br>
          <%= link_to "edit", edit_user_path(user.id) %>
        </div>
      </li>
    <% end %>
  </ul>

</div>


