<%= form_with model: @user, url: user_path do |form| %>

  <h1>edit user </h1>
  <h2> username: <%= @user.username %> </h2>
  <h2> email: <%= @user.email %> </h2>
  <h2> password: <%= @user.password %> </h2>
  <%= form.label "Username" %>
  <%= form.text_field "username" %>
  <%= form.submit "Update" %>

<% end %>
